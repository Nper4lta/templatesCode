#!/bin/bash
# Last Modified: 2023-02-01
source templateFunct

HTMLheader(){
	filename=$1
	creator=$2
	echo "<!DOCTYPE html>" >> $filename
	echo "<html lang=\"en\" xmlns="\"https://www.w3.org/1999/xhtml\">" >> $filename
	echo "<!--" >> $filename

	creator_name $filename $creator
	
	echo "you can run this using the URL: " >> $filename
	echo "-->" >> $filename
	echo "	<head>" >> $filename
	echo "		<meta charset=\"UTF-8\">" >> $filename
	echo "			<title> INSERT_TITLE </title>" >> $filename
	echo "	</head>" >> $filename
	echo >> $filename
	echo "</html>" >> $filename
}
